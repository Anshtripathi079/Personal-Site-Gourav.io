{
  "name": "personal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "//start": "starts Next.js in development mode",
    "start": "next dev",
    "start:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "//prod": "starts Next.js in production server",
    "start:prod": "next start",
    "deploy": "npm run tsc && dotenv -e .env.prod npm run export && firebase deploy -P prod --only hosting",
    "//": "//-internal commands-//",
    "//dont use NODE_ENV": "https://github.com/vercel/next.js/blob/master/errors/non-standard-node-env.md",
    "build": "next build",
    "export": "next build && next export",
    "deploy-lint": "npm run lint && npm run tsc && npm run export && firebase deploy --only hosting",
    "tsc": "tsc",
    "lint": "eslint ./ -c .eslintrc.json --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint ./components/** ./pages/** -c .eslintrc.json --fix --ext .js,.jsx,.ts,.tsx,.scss"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.2.0",
    "next": "10.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "^1.9.6"
  },
  "devDependencies": {
    "@types/node": "^14.14.8",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "typescript": "^4.0.5"
  }
}
