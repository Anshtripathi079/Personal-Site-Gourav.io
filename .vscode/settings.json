{
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.validate.enable": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,

  "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
      // "statusBar.background": "#0a867b",
      // "statusBar.foreground": "#e7e7e7",
      "titleBar.activeBackground": "#9a3412"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": false,
  "githubPullRequests.pushBranch": "always",
  "githubPullRequests.defaultMergeMethod": "squash",
  "git.postCommitCommand": "sync",
  "search.exclude": {
      ".next": true,
      ".output": true,
      ".vercel": true,
      "**/node_modules": true,
      ".plasmo": true,
      "build": true
  },
  "search.seedWithNearestWord": true,
  "search.showLineNumbers": true,
  "search.useIgnoreFiles": false,
  "search.collapseResults": "auto",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit",
      "source.addMissingImports": "explicit"
  },
  "reactExtract.typeDeclaration": "inline",
  "tailwindCSS.experimental.classRegex": [
      ["Classes \\=([^;]*);", "'([^']*)'"],
      ["Classes \\=([^;]*);", "\"([^\"]*)\""],
      ["Classes \\=([^;]*);", "\\`([^\\`]*)\\`"],
      ["Styles \\=([^;]*);", "'([^']*)'"],
      ["Styles \\=([^;]*);", "\"([^\"]*)\""],
      ["Styles \\=([^;]*);", "\\`([^\\`]*)\\`"],
      ["Cls \\=([^;]*);", "'([^']*)'"],
      ["Cls \\=([^;]*);", "\"([^\"]*)\""],
      ["Cls \\=([^;]*);", "\\`([^\\`]*)\\`"]
  ],
  "tailwindCSS.classAttributes": ["class", "className", "ngClass", "class:list", ".*Styles", ".*Classes", ".*Cls"],

  "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enableFiletypes": ["!json"],
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.useGitignore": false,
  "cSpell.words": [
      "BGSW",
      "Blazingly",
      "Btns",
      "Chatbox",
      "chatgptweb",
      "clsx",
      "convo",
      "convos",
      "couldn",
      "couldnt",
      "Davinci",
      "deauthorized",
      "Discardable",
      "Divs",
      "duplicacy",
      "emailbox",
      "endregion",
      "Firestore",
      "FRESHDESK",
      "Frontmatter",
      "gitbranch",
      "Gorv",
      "Gourav",
      "Goyal",
      "Goyl",
      "hackernews",
      "idempotency",
      "inputbox",
      "IPINFO",
      "isnotempty",
      "keyframes",
      "Kolkata",
      "logid",
      "MAILGUN",
      "minimise",
      "mixspace",
      "mixspaces",
      "MULTIBODY",
      "myip",
      "openai",
      "optimise",
      "optmised",
      "pageview",
